{
  "description": "controls how audit info should be logged. FullAudit is not recommended on produciton if performance is important",
  "enableFullAudit": false,
  "enableSimpleAudit": false,
  "full" : {
    "enableMask": true,
    "request": {
      "headers": [
        "contentType",
        "characterEncoding"
      ],
      "cookies": true,
      "queryParameters": true,
      "body": true
    },
    "response": {
      "headers": true,
      "cookies": true,
      "body": true,
      "statusCode": true,
      "contentLength": true
    }
  },
  "simple": {
    "statusCode": true,
    "responseTime": true,
    "headers": [
      "correlationId",
      "traceabilityId",
      "clientId",
      "userId",
      "scopeClientId",
      "endpoint"
    ]
  }
}